{
  "name": "aliens-ts",
  "version": "1.0.0",
  "publisher": "doug",
  "license": "MIT",
  "engines": {
    "vscode": "^1.25.0"
  },
  "main": "./out/extension",
  "activationEvents": [
    "onCommand:doug.aliens"
  ],
  "contributes": {
    "commands": [
      {
        "command": "doug.aliens",
        "title": "Aliens!"
      }
    ]
  },
  "scripts": {
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc",
    "compile:views": "webpack --mode development",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc --watch",
    "watch:views": "webpack --watch --mode development",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "vscode": "^1.1.18"
  },
  "devDependencies": {
    "@types/node": "^10.5.2",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "css-loader": "^1.0.0",
    "less": "^3.7.0",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.3",
    "style-loader": "^0.21.0",
    "styled-components": "^3.3.3",
    "ts-import-plugin": "^1.5.4",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  }
}
